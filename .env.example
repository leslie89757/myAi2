# OpenAI API密钥 - 替换为您自己的密钥
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI API替代端点 - 如果您需要使用替代API端点
# OPENAI_BASE_URL=https://api.openai-proxy.com/v1

# 网络代理设置 - 如果您的网络需要代理才能访问OpenAI API
# HTTP_PROXY=http://127.0.0.1:7890
# HTTPS_PROXY=http://127.0.0.1:7890

# 模拟模式 - 设置为true时，系统将使用本地模拟响应代替实际调用OpenAI API
# MOCK_OPENAI=true

# API 鉴权密钥 - 格式: key1:name1,key2:name2
# 示例: API_KEYS=abcd1234:开发团队,efgh5678:测试团队
API_KEYS=your_api_key_here:your_name_here

# PostgreSQL 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=chatgpt_api

# JWT 密钥 (用于用户认证)
JWT_SECRET=your_jwt_secret_key_here

# API 密钥生成密钥
API_KEY_SECRET=your_api_key_secret_here

# 服务器端口
PORT=3001

# 刷新令牌密钥 (用于更新访问令牌)
REFRESH_SECRET=your_refresh_token_secret_here
