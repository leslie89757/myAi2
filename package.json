{
  "name": "demo-ts-api",
  "version": "1.0.0",
  "description": "A TypeScript Node.js API deployed on Vercel",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc && npm run copy-files",
    "copy-files": "mkdir -p dist/public && cp -r src/public/* dist/public/",
    "vercel-build": "npm run build",
    "prestart": "npm run build",
    "dev": "nodemon --exec ts-node src/index.ts",
    "db:setup": "psql -f scripts/setup-db.sql",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@langchain/community": "^0.3.41",
    "@langchain/openai": "^0.5.6",
    "@types/express": "^4.17.17",
    "bcrypt": "^5.1.1",
    "chromadb": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.24",
    "mammoth": "^1.9.0",
    "multer": "^1.4.5-lts.2",
    "openai": "^4.28.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.15.5",
    "redoc-express": "^2.1.0",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.96",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.19",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.11.14",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=18.x"
  }
}
