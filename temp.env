# OpenAI API密钥 - 替换为您自己的密钥
OPENAI_API_KEY=your_openai_api_key_here

# API 鉴权密钥 - 格式: key1:name1,key2:name2
# 示例: API_KEYS=abcd1234:开发团队,efgh5678:测试团队
API_KEYS=test_key:test_user

# PostgreSQL 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=guzhenqiang
DB_PASSWORD=
DB_NAME=chatgpt_api

# Prisma 数据库连接URL
DATABASE_URL=postgresql://guzhenqiang:@localhost:5432/chatgpt_api

# JWT 密钥 (用于用户认证)
JWT_SECRET=jwt_secret_key_for_development

# API 密钥生成密钥
API_KEY_SECRET=api_key_secret_for_development

# 服务器端口
PORT=3001
